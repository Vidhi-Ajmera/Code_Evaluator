{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "build": {
      "env": {
        "PYTHON_VERSION": "3.9"
      },
      "command": "echo 'No build required'",
      "output": "."
    }
  }